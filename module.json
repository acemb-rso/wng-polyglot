{
	"id": "wng-CombatExtender",
	"type": "module",
	"title": "Wrath & Glory - Combat Extender",
	"description": "Adds melee/ranged combat options to the Wrath & Glory weapon roll dialog.",
        "version": "0.8.6",
	"authors": [
		{
			"name": "acemb-rso",
			"discord": "Goldwalker"
		}
	],
	"compatibility": {
		"minimum": "12",
		"verified": "13"
	},
        "esmodules": [
                "scripts/combat-options.js",
                "scripts/combat-options/constants.js",
                "scripts/combat-options/dialog.js",
                "scripts/combat-options/engagement.js",
                "scripts/combat-options/logging.js",
                "scripts/combat-options/measurement.js",
                "scripts/combat-options/permissions.js",
                "scripts/combat-options/turn-effects.js",
                "scripts/contested-roll.js"
        ],
        "styles": ["styles/combat-options.css"],
        "languages": [{ "lang": "en", "name": "English", "path": "lang/en.json" }],
        "templates": [
                "templates/combat-options.hbs",
                "templates/partials/co-checkbox.hbs",
                "templates/partials/co-select.hbs"
        ],
	"url": "https://github.com/your-org/wng-combat-extender",
	"manifest": "https://raw.githubusercontent.com/acemb-rso/wng-CombatExtender/main/module.json",
	"download": "https://github.com/acemb-rso/wng-CombatExtender/archive/refs/heads/main.zip",
	"relationships": {
    "systems": [
      {
        "id": "wrath-and-glory",
        "type": "system",
        "compatibility": {
          "minimum": "6",
          "verified": "6"
        }
      }
    ],
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "1.12.0.0",
          "verified": "1.13.4.0"
        }
      }
    ]
  },
  "flags": {}
}
